<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">
           
      <bean id="message" class="com.korolija.model.Message" init-method="init" destroy-method="destroy">
      <property name="messageId" value="101"></property>
      <property name="messageName" value="Hello World!"></property>
      </bean>
      
      <bean class="com.korolija.processor.MyBeanPostProcessor"></bean>
      <bean class="com.korolija.processor.MyBeanPostProcessor2"></bean>

          
           
 </beans>