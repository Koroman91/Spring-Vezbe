<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd">
           
          <bean id="orgInfo" class="com.korolija.info.OrgInfo">
          
          <property name="studArr">
          <array>
          <ref bean="student1"/>
          <ref bean="student2"/>
          <ref bean="student3"/>
          <ref bean="student1"/>
          </array>
          </property>
          
           <property name="studList">
          <list>
          <ref bean="student1"/>
          <ref bean="student2"/>
          <ref bean="student3"/>
          <ref bean="student1"/>
          </list>
          </property>
          
          <property name="studSet">
          <set>
          <ref bean="student1"/>
          <ref bean="student2"/>
          <ref bean="student3"/>
          <ref bean="student1"/>
          </set>
          </property>
          
          </bean>
                    
      <bean id="student1" class="com.korolija.model.Student">
      <property name="studentId" value="0001"></property>
      <property name="studentName" value="Stefan Korolija"></property>
      <property name="email" value="korolija91@gmail.com"></property>
      <property name="gender" value="Male"></property>
      </bean>
        
       <bean id="student2" class="com.korolija.model.Student">
      <property name="studentId" value="0002"></property>
      <property name="studentName" value="Ivan Korolija"></property>
      <property name="email" value="ivankorolija91@gmail.com"></property>
      <property name="gender" value="Male"></property>
      </bean>
      
      <bean id="student3" class="com.korolija.model.Student">
      <property name="studentId" value="0003"></property>
      <property name="studentName" value="Miodrag Korolija"></property>
      <property name="email" value="miodragkorolija@gmail.com"></property>
      <property name="gender" value="Male"></property>
      </bean>  
          
           
 </beans>